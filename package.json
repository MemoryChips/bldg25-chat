{
  "name": "chat-6-demo",
  "version": "1.6.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config ./proxy.json",
    "start-secure": "ng serve -c secure --proxy-config ./proxy-secure.json --ssl --ssl-key key.pem --ssl-cert cert.pem",
    "pre-load-app-data": "ts-node ./server/database/pre-load-app-data.ts",
    "start-server": "echo '*** Be sure you have run build with ctrl-shift-b ***' && node dist/server.js",
    "build-server": "./node_modules/.bin/tsc -p ./server/tsconfig.json",
    "build": "ng build --prod --aot && npm run build-server",
    "test": "ng test --watch",
    "api-test": "jasmine-ts server/__test__/* --config=server/__test__/jasmine.json",
    "api-test-single": "jasmine-ts server/__test__/login* --config=server/__test__/jasmine.json",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "npm run build",
    "dep-api-test": "ts-node node_modules/jasmine/bin/jasmine  server/__test__/*",
    "dep-pre-load-app-data-local": "ts-node ./server/database/pre-load-app-data.ts --dbHost localhost --dbPort 6379 --dbAuth this_should_be_a_secret_authcode",
    "dep-start-server-local": "echo '*** Be sure you have run build with ctrl-shift-b ***' && node server/dist/out-tsc/server.js --dbHost localhost --dbPort 6379 --dbAuth this_should_be_a_secret_authcode",
    "dep-server": "./node_modules/.bin/nodemon  -w ./server --ext \".ts\" --exec \"npm run start-server\"",
    "dep-start-server-prod": "echo '*** Be sure you have built front end app and run server build with ctrl-shift-b ***' && node dist/server.js --secure --prod",
    "dep-start-image-server": "./node_modules/.bin/ts-node ./server/image-server.ts",
    "dep-start-redisdb": "redis-server ./server/database/redis.conf",
    "dep-pre-load-user-data": "ts-node ./server/database/pre-load-user-data.ts",
    "dep-pre-load-app": "npm run pre-load-user-data && npm run pre-load-app-data",
    "dep-redis-cli": "redis-cli -p 6379"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.10",
    "@angular/cdk": "^6.4.7",
    "@angular/cli": "^6.2.5",
    "@angular/common": "^6.1.10",
    "@angular/compiler": "^6.1.10",
    "@angular/compiler-cli": "^6.1.10",
    "@angular/core": "^6.1.10",
    "@angular/forms": "^6.1.10",
    "@angular/http": "^6.1.10",
    "@angular/material": "^6.4.7",
    "@angular/platform-browser": "^6.1.10",
    "@angular/platform-browser-dynamic": "^6.1.10",
    "@angular/router": "^6.1.10",
    "@types/mongodb": "^3.1.12",
    "argon2": "^0.19.3",
    "argon2-ffi": "^1.1.0",
    "autolinker": "^1.7.1",
    "bldg25-chat": "file:../bldg25-chat-dev/bldg25-chat-2.2.0.tgz",
    "bldg25-chat-server": "file:../bldg25-chat-dev/bldg25-chat-server-2.2.0.tgz",
    "body-parser": "^1.17.2",
    "bootstrap": "^4.1.3",
    "command-line-args": "^5.0.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express": "^4.16.4",
    "express-http-proxy": "^1.4.0",
    "font-awesome": "^4.7.0",
    "hammerjs": "^2.0.8",
    "jsonwebtoken": "^8.3.0",
    "moment": "^2.18.1",
    "mongodb": "^3.1.8",
    "ng2-validation": "^4.2.0",
    "password-validator": "^4.0.0",
    "redis": "^2.8.0",
    "rxjs": "^6.3.3",
    "typescript": "2.7.2",
    "ws": "^6.1.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.8.5",
    "@angular/language-service": "^6.1.10",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.16.0",
    "@types/form-data": "^2.2.1",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/jsonwebtoken": "^7.2.7",
    "@types/node": "^10.12.0",
    "@types/node-fetch": "^2.1.2",
    "@types/redis": "^2.8.7",
    "@types/ws": "^6.0.1",
    "codelyzer": "^4.5.0",
    "concurrently": "^4.0.1",
    "jasmine": "^3.2.0",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "karma": "^3.0.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^1.3.1",
    "node-fetch": "^2.2.0",
    "nodemon": "^1.18.4",
    "protractor": "^5.4.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0"
  },
  "engines": {
    "node": "10.4.0",
    "npm": "6.2.0"
  }
}
